<div>
  <div class="header">
    <div class="mdc-select version-selector" role="listbox">
      <div class="mdc-select__surface" tabindex="0">
        <div class="mdc-select__label mdc-select__label--float-above">API Version</div>
        <div class="mdc-select__selected-text"></div>
        <div class="mdc-select__bottom-line"></div>
      </div>
      <div class="mdc-menu mdc-select__menu">
        <ul class="mdc-list mdc-menu__items">
          {{#each (docVersions path)}}
          <li class="mdc-list-item" role="option" tabindex="0" data-version={{this}}
            data-link={{url_for (concat (apiRoot ../path) this)}}/ {{#if (eq ../page.title this)}} aria-selected{{/if}}>
            {{this}}
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
    <div class="mdc-select backend-selector" role="listbox">
      <div class="mdc-select__surface" tabindex="0">
        <div class="mdc-select__label mdc-select__label--float-above">Backend</div>
        <div class="mdc-select__selected-text backend-selector"></div>
        <div class="mdc-select__bottom-line"></div>
      </div>
      <div class="mdc-menu mdc-select__menu">
        <ul class="mdc-list mdc-menu__items">
          {{#if (isWebGPUSupported page.title)}}
          <li class="mdc-list-item" role="option" tabindex="0" data-version={{this}} backend-name='webgpu' aria-selected>webgpu</li>
          {{/if}}
          {{#if (isWebGLSupported page.title)}}
          <li class="mdc-list-item" role="option" tabindex="0" data-version={{this}} backend-name='webgl'>webgl</li>
          {{/if}}
          {{#if (isWasmSupported page.title)}}
          <li class="mdc-list-item" role="option" tabindex="0" data-version={{this}} backend-name='wasm'>wasm</li>
          {{/if}}
          <li class="mdc-list-item" role="option" tabindex="0" data-version={{this}} backend-name='cpu' {{#unless (isWebGPUSupported page.title)}}aria-selected {{/unless}}>cpu</li>
        </ul>
      </div>
    </div>
  </div>
  {{#with (getApi page.site.data page.title path)}} {{#headings}} <div class="heading">
    <div class="symbol">
      <a class="heading-name" href="#{{name}}">{{name}}</a>
    </div>

    {{#subheadings}}
    <div class="subheading">
      <div class="symbol">
        <a class="subheading-name" href="#{{../name}}-{{name}}">{{name}}</a>
      </div>

      {{#symbols}}
      <div class="symbol">
        <a href="#{{urlHash}}">{{displayName}}</a>
        {{#if isClass}}
        <div class="method-list">
          {{#methods}}
          <div class="method">
            <a href="#{{urlHash}}">.{{symbolName}}</a>
          </div>
          {{/methods}}
        </div>
        {{/if}}
      </div>
      {{/symbols}}
    </div>
    {{/subheadings}}

  </div>
  {{/headings}}
  {{/with}}
</div>
